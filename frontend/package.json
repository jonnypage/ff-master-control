{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "if test -f ../src/schema.gql; then npm run codegen; else echo \"Skipping codegen: ../src/schema.gql not found (expected on Railway when service root is /frontend)\"; fi",
    "build": "tsc -b && vite build",
    "codegen:check": "test -f ../src/schema.gql || (echo 'Error: schema.gql not found' && exit 1)",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "codegen": "npm run codegen:check && graphql-codegen --config codegen.yml --debug && npm run codegen:fix-exports",
    "codegen:fix-exports": "node scripts/fix-codegen-exports.mjs",
    "codegen:watch": "graphql-codegen --config codegen.yml --watch"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/client-preset": "^5.2.0",
    "@graphql-codegen/typescript": "^5.0.5",
    "@graphql-codegen/typescript-operations": "^5.0.5",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-query": "^5.90.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitest/ui": "^4.0.13",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "graphql-request": "^7.3.4",
    "jsdom": "^27.2.0",
    "lucide-react": "^0.554.0",
    "msw": "^2.12.2",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-router-dom": "^7.9.6",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^4.0.13",
    "workbox-window": "^7.4.0",
    "zod": "^4.1.12",
    "@vitejs/plugin-react": "^5.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
